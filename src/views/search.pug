extend layouts/main
include mixins/videoBlock

block content
  .search__header 
    h3 Searching By: #{searchingBy}
  .search__videos
    if (videos.length === 0)
      h4 There's no video matches with #{searchingBy}


    each item in videos
      +videoBlock({
        id: item.id, 
        title: item.title,
        views: item.views, 
        fileUrl: item.fileUrl
      })
    